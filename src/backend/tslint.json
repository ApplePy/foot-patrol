{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "no-console": false,
      "object-literal-sort-keys": {
        "options": ["ignore-case", "match-declaration-order"],
        "severity": "warning"
      },
      "trailing-comma": false,
      "no-trailing-whitespace": {
        "options": ["ignore-jsdoc"],
        "severity": "warning"
      }
    },
    "rulesDirectory": []
}